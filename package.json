{
    "name": "mix-skeleton",
    "version": "1.2.0",
    "main": "index.js",
    "repository": "git@bitbucket.org:mixinternet/mix-skeleton.git",
    "author": "",
    "license": "MIT",
    "scripts": {
        "clean:admin": "shx rm -rf ./www/assets/css/admin/* && shx rm -rf ./www/assets/js/admin/*",
        "clean:site": "shx rm -rf ./www/assets/css/site/* && shx rm -rf ./www/assets/js/site/*",
        "dev:admin": "cross-env NODE_ENV=\"development\" webpack --config modules/admin/webpack.config.js",
        "dev:site": "cross-env NODE_ENV=\"development\" webpack-dev-server --hot",
        "dev": "npm run dev:site",
        "build:site": "npm run clean:site && cross-env NODE_ENV=\"production\" webpack",
        "build:admin": "npm run clean:admin && cross-env NODE_ENV=\"production\" webpack --config modules/admin/webpack.config.js",
        "build": "npm run build:site && npm run build:admin",
        "lint:js": "eslint ./application/src-assets/scripts ./.webpack ./*.js -f table --ext .js --fix",
        "lint:scss": "stylelint ./application/src-assets/styles/**/*.scss ./modules/*/src-assets/**/*.scss --syntax scss --fix",
        "lint": "npm run lint:js && npm run lint:scss"
    },
    "dependencies": {
        "@babel/runtime": "^7.1.5",
        "axios": "^0.18.0",
        "bigslide": "^0.12.1",
        "bootstrap": "^4.1.1",
        "bootstrap-vue": "^2.0.0-rc.11",
        "clip-one": "git+https://bitbucket.org/mixinternet/mix-clip-one.git#1.5.4",
        "font-awesome": "^4.7.0",
        "headroom.js": "^0.9.4",
        "imagesloaded": "^4.1.4",
        "jquery-mask-plugin": "^1.14.16",
        "lg-fullscreen": "^1.0.1",
        "lg-hash": "^1.0.4",
        "lg-thumbnail": "^1.1.0",
        "lg-video": "^1.2.2",
        "lg-zoom": "^1.1.0",
        "lightgallery": "^1.6.11",
        "lodash": "^4.17.10",
        "vanilla-lazyload": "^10.4.2",
        "vee-validate": "^2.0.9",
        "vue": "^2.5.16",
        "vue-headroom": "^0.8.1",
        "vue-notification": "^1.3.12",
        "vue-router": "^3.0.1",
        "vue-scrollto": "^2.11.0",
        "vuex": "^3.0.1"
    },
